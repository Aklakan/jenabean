<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.example.org/geonamesext.owl#"
     xml:base="http://www.example.org/geonamesext.owl"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:ontology="http://www.geonames.org/ontology#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#">
     
    <owl:Ontology rdf:about="">
        <owl:imports rdf:resource="http://www.geonames.org/ontology"/>
    </owl:Ontology>
    <owl:DatatypeProperty rdf:ID="population">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:subPropertyOf rdf:resource="http://www.geonames.org/ontology#population"/>
    </owl:DatatypeProperty>
</rdf:RDF>
